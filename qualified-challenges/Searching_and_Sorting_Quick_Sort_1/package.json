{
  "name": "quick-sort",
  "version": "1.0.0",
  "description": "Practice for searching and sorting quick sort.",
  "main": "src/main.js",
  "directories": {
    "doc": "docs",
    "test": "__tests__"
  },
  "scripts": {
    "docker:build": "docker image build . -t thinkful-ed/searching-sorting-06-quick-sort",
    "docker:run": "docker run --rm -it -p 3000:3000 thinkful-ed/searching-sorting-06-quick-sort",
    "docker:stop": "docker stop $(docker ps -q)",
    "docker:test": "docker run -t thinkful-ed/searching-sorting-06-quick-sort npm test",
    "start": "node src/main.js",
    "start:solution": "npm run -it docker:build && npm run docker:run",
    "test": "jest",
    "test:solution": "npm run docker:build && npm run docker:test",
    "test:watch": "jest --watch"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "jest": "^26.6.3"
  }
}
